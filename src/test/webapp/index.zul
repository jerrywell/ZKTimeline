<zk>
	<div></div>
	<zscript><![CDATA[
	    import org.zkoss.zk.timeline.*;
        List eee = new ArrayList();
        TimelineEvent e1 = new TimelineEvent();
        e1.setTitle("e1");
        e1.setStartDate(new Date());
        e1.setStopDate(new Date());
        TimelineEvent e2 = new TimelineEvent();
        e2.setTitle("e2");
        e2.setStartDate(new Date());
        eee.add(e1);
        eee.add(e2);
        Date d = new Date();
    ]]></zscript>
 	<window title="Hello World!!" border="normal">
		<timeline id="myComp" pivot="${d}" timelineEvents="${eee}" width="1200px" height="200px"/>
	</window>
	
	<datebox value="${d }"></datebox>
</zk>